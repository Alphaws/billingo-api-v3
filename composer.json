{
    "name": "alphaws/billingo-api-v3",
    "description": "Billingo Invoice aPI clone from Swagger Hub",
    "keywords": [
        "swagger",
        "php",
        "sdk",
        "api",
        "billingo",
        "clone"
    ],
    "homepage": "https://app.swaggerhub.com/apis/Billingo/Billingo/3.0.14",
    "license": "proprietary",
    "authors": [
        {
            "name": "Swagger and contributors",
            "homepage": "https://github.com/swagger-api/swagger-codegen"
        }
    ],
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~1.12"
    },
    "autoload": {
        "psr-4": { "Swagger\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Swagger\\Client\\" : "test/" }
    }
}
